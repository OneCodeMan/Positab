{
  "manifest_version": 2,

  "name": "Positab",
  "description": "This extension delivers positivity with every new tab.",
  "version": "1.3",

  "browser_action": {
    "default_icon": "positab-img.png",
    "default_popup": "popup.html"
  },

  "chrome_url_overrides": {
    "newtab": "popup.html"
  },

  "permissions": [
    "activeTab",
    "tabs"
  ]
}
